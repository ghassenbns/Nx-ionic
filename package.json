{
  "name": "concordia-nx-ionic",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "fit-start": "nx serve fitdrive-shell --skip-nx-cache",
    "fit-start_local_api": "nx serve fitdrive-shell -c=local_api --skip-nx-cache",
    "fit-build": "nx build fitdrive-shell --prod --skip-nx-cache",
    "fit-test": "nx test fitdrive-shell",
    "fit-lint": "nx lint fitdrive-shell",
    "fit-ci-test": "nx run-many --target=test --parallel=1 fitdrive-shell",
    "fit-ci-all-test": "npm run fit-ci-test -- --all --skip-nx-cache",
    "fit-ci-all-test-coverage": "npm run fit-ci-test -- --all --codeCoverage=true --skip-nx-cache",
    "fit-ci-lint": "nx run-many --target=lint --parallel",
    "fit-ci-all-lint": "npm run fit-ci-lint -- --all --skip-nx-cache",
    "fit-ci-e2e-watch": "as-a DEV nx run-many --target=e2e --parallel=1 fitdrive-shell --watch --skip-nx-cache",
    "fit-ci-e2e-test": "as-a DEV nx run-many --target=e2e --parallel=1 fitdrive-shell --skip-nx-cache",
    "fit-analyze": "nx build fitdrive-shell --prod --stats-json --skip-nx-cache && webpack-bundle-analyzer dist/apps/fitdrive-shell/stats.json",
    "con-start": "nx serve app-concordia-ui --skip-nx-cache",
    "con-start_local_api": "nx serve app-concordia-ui -c=local_api --skip-nx-cache",
    "con-build": "nx build app-concordia-ui --prod --skip-nx-cache",
    "con-test": "nx test app-concordia-ui",
    "con-lint": "nx lint app-concordia-ui",
    "con-ci-test": "nx run-many --target=test --parallel=1 app-concordia-ui",
    "con-ci-all-test": "npm run con-ci-test -- --all --skip-nx-cache",
    "con-ci-all-test-coverage": "npm run con-ci-test -- --all --codeCoverage=true --skip-nx-cache",
    "con-ci-lint": "nx run-many --target=lint --parallel",
    "con-ci-all-lint": "npm run con-ci-lint -- --all --app-concordia-ui",
    "con-ci-e2e-watch": "as-a DEV nx run-many --target=e2e --parallel=1 app-concordia-ui --watch --skip-nx-cache",
    "con-ci-e2e-test": "as-a DEV nx run-many --target=e2e --parallel=1 app-concordia-ui --skip-nx-cache",
    "con-analyze": "nx build app-concordia-ui --prod --stats-json --skip-nx-cache && webpack-bundle-analyzer dist/apps/app-concordia-ui/stats.json",
    "prepare": "husky"
  },
